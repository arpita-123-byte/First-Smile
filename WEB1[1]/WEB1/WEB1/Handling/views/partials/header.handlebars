<header>
    <div class="logo">
        <img src="{{logoUrl}}" alt="FirstSmile Logo" />
    </div>
    <nav>
        <ul>
            {{#each navLinks}}
            <li><a href="{{href}}">{{text}}</a></li>
            {{/each}}
        </ul>
    </nav>
    <div class="search-contact">
        <input type="text" placeholder="Search..." />
        <div class="icon-container">
            {{#each icons}}
            <a href="{{href}}" class="icon"><i class="{{iconClass}}"></i></a>
            {{/each}}
        </div>
    </div>
</header>


